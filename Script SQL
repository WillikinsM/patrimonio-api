Script de criação Banco de dados



create table marca (
       marca_id integer generated by default as identity,
        nome varchar(255),
        patrimonio_numero_tombo integer,
        primary key (marca_id)
    )
    
    
    create table patrimonio (
       numero_tombo integer generated by default as identity,
        descricao varchar(255),
        nome varchar(255),
        primary key (numero_tombo)
    )
    
    
    create table usuario (
       id integer generated by default as identity,
        email varchar(255),
        nome varchar(255),
        senha varchar(255),
        primary key (id)
    )
    
      
	alter table marca 
       add constraint FK_numeroTombo 
       foreign key (patrimonio_numero_tombo) 
       references patrimonio
	   
